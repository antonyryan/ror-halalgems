<div class="col-sm-6 col-md-3">
	<fieldset>			
		<div class="thumbnail">
			<%= cl_image_tag(f.object.photo_url, :width => 280, :height => 185) %>
			<div class="caption">
				<%= f.hidden_field :_destroy %>
				<%= link_to 'remove', '#', class: "remove_fields" %>
			</div>
		</div>
	</fieldset>
</div>